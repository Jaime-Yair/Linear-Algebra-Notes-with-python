   "source": [
    "# A function\n",
    "\n",
    "def zero_matrix(rows, cols):\n",
    "    Z = [[0 for j in range(cols)] for i in range(rows)]\n",
    "    return Z\n",
    "\n",
    "\n",
    "zero_matrix(4, 3)"
   ]

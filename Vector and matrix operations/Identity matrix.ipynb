   "metadata": {},
   "outputs": [],
   "source": [
    "# A function\n",
    "\n",
    "\n",
    "def identity_matrix(n):\n",
    "    I = [[0 for j in range(n)] for i in range(n)]\n",
    "    for i in range(len(I)):\n",
    "        I[i][i]=1\n",
    "    return I"
   ]
